package com.superdata.pm.entity;

import android.widget.TextView;

/**
 * ExpandableListView 父列表实体
 * @author kw
 *
 */
public class ExpandableGroup {

	private TextView package_name; //工作包名称
	private TextView package_startdata; //工作包开始时间
	private TextView package_enddata; //工作包结束时间
	private TextView package_duration; //工作包持续时间
	private TextView package_person; //工作包负责人
	
	public ExpandableGroup(TextView package_name, TextView package_startdata,
			TextView package_enddata, TextView package_duration,
			TextView package_person) {
		super();
		this.package_name = package_name;
		this.package_startdata = package_startdata;
		this.package_enddata = package_enddata;
		this.package_duration = package_duration;
		this.package_person = package_person;
	}
	

	public ExpandableGroup() {
		super();
	}


	public TextView getPackage_name() {
		return package_name;
	}

	public void setPackage_name(TextView package_name) {
		this.package_name = package_name;
	}

	public TextView getPackage_startdata() {
		return package_startdata;
	}

	public void setPackage_startdata(TextView package_startdata) {
		this.package_startdata = package_startdata;
	}

	public TextView getPackage_enddata() {
		return package_enddata;
	}

	public void setPackage_enddata(TextView package_enddata) {
		this.package_enddata = package_enddata;
	}

	public TextView getPackage_duration() {
		return package_duration;
	}

	public void setPackage_duration(TextView package_duration) {
		this.package_duration = package_duration;
	}

	public TextView getPackage_person() {
		return package_person;
	}

	public void setPackage_person(TextView package_person) {
		this.package_person = package_person;
	}
	
	
}
