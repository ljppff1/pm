                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_expenses"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:layout_marginLeft="9dp"
                    android:text="费用预算："
                    android:textColor="#000"
                    android:textSize="15sp" />
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_expense"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_expenses"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="12sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_material"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_expense"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_materials"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:layout_marginLeft="9dp"
                    android:text="材料预算："
                    android:textColor="#000"
                    android:textSize="15sp" />
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_material"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_materials"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="12sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_job"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_material"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_jobs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:layout_marginLeft="9dp"
                    android:text="人力预算："
                    android:textColor="#000"
                    android:textSize="15sp" />
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_job"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_jobs"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="12sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_state"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_job"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_states"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="项目状态："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_states"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_creater"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_state"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_creaters"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:layout_marginLeft="8dp"
                    android:text="立 项 人："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_creater"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_creaters"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_createtime"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_creater"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_createtimes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="立项日期："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_createtime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_createtimes"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_description"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_createtime"
                android:background="@drawable/preference_last_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_descriptions"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="项目说明："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_descriptions"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
