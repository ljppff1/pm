<?xml version="1.0" encoding="utf-8"?>
<!-- 项目详情 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/list_main"
    android:orientation="vertical" >
    
    <include
        android:id="@+id/include"
        layout="@layout/top_title_projectdetail" />
    
    <ScrollView 
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        >
        
        <RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            >
            
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_number"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/preference_first_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_numbers"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="项目编号："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/tv_projectmanager_check_detail_numbers"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_name"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_number"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_names"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="项目名称："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_names"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_startdata"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_name"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_startdatas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="开始日期："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_startdata"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_startdatas"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_enddata"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_startdata"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_enddatas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="结束日期："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_enddata"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_enddatas"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_type"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_enddata"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_types"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="项目类型："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_types"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_manager"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_type"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_managers"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="项目经理："
                    android:textColor="#000"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_manager"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_managers"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_budget"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_check_detail_manager"
                android:background="@drawable/preference_item"
                android:clickable="true"
                android:gravity="center_vertical"
                >
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_budgets"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="资金预算："
                    android:textColor="#000"
                    android:textSize="17sp" />
                
                <TextView
                    android:id="@+id/tv_projectmanager_check_detail_budget"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_projectmanager_check_detail_budgets"
                    android:padding="8dp"
                    android:text=""
                    android:textColor="#000"
                    android:textSize="14sp" />
                
            </RelativeLayout>
            
            <RelativeLayout 
                android:id="@+id/rl_check_detail_expense"
