package com.superdata.pm.entity;

import java.io.Serializable;

public class PEmployee implements Serializable{

	/**
	 * @Fields serialVersionUID 序列化ID
	 */
	private static final long serialVersionUID = -6407279633065405031L;

	private Integer projectId;		//项目ID
	
	private String projectName;		//项目名称
	
	private Integer empId;			//员工ID
	
	private String empName;			//员工姓名
	
	private String empCode;			//员工编号
	
	private String empDept;			//员工部门
	
	private Integer positionId;		//职位ID
	
	private String positionName;	//职位名称
	
	private String remark;			//备注
	
	private Integer isCreator;		//是否立项人 1:是 0:否
	
	private String isCreatorName;	//是否立项人 
	
	private Integer closed;			//是否停止参与 1:正常 0:停止参与
	
	private String closedName;		//是否停止参与
	
	private String closeStatu;		//用于页面的状态显示 
	
	private Integer roleID;			//角色ID
	
	private String roleName;		//角色名称
	
	private String positionRoleName;//职位角色名称
	

	public PEmployee(String empName, String empCode, String empDept,
			String positionName, String remark, String isCreatorName,
			String closedName, Integer empId, Integer projectId) {
		super();
		this.empName = empName;
		this.empCode = empCode;
		this.empDept = empDept;
		this.positionName = positionName;
		this.remark = remark;
		this.isCreatorName = isCreatorName;
		this.closedName = closedName;
		this.empId = empId;
		this.projectId = projectId;
	}
	
	public PEmployee(String empName, String empCode, String empDept,
			String positionName, String remark, String isCreatorName,
			String closedName) {
		super();
		this.empName = empName;
		this.empCode = empCode;
		this.empDept = empDept;
		this.positionName = positionName;
		this.remark = remark;
		this.isCreatorName = isCreatorName;
		this.closedName = closedName;
	}
	
	public PEmployee() {
	}


	public Integer getProjectId() {
		return projectId;
	}

	public void setProjectId(Integer projectId) {
		this.projectId = projectId;
	}

	public String getProjectName() {
		return projectName;
	}

	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}

	public Integer getEmpId() {
		return empId;
	}

	public void setEmpId(Integer empId) {
		this.empId = empId;
	}

	public String getEmpName() {
		return empName;
	}

	public void setEmpName(String empName) {
		this.empName = empName;
	}

	public String getEmpCode() {
		return empCode;
	}

	public void setEmpCode(String empCode) {
		this.empCode = empCode;
	}

	public String getEmpDept() {
		return empDept;
	}

	public void setEmpDept(String empDept) {
		this.empDept = empDept;
	}

	public Integer getPositionId() {
		return positionId;
	}

	public void setPositionId(Integer positionId) {
		this.positionId = positionId;
	}

	public String getPositionName() {
		return positionName;
	}

	public void setPositionName(String positionName) {
		this.positionName = positionName;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Integer getIsCreator() {
		return isCreator = isCreator ==null?0:isCreator;
	}

	public void setIsCreator(Integer isCreator) {
		this.isCreator = isCreator ==null?0:isCreator;
	}

	public String getIsCreatorName() {
		return isCreatorName;
	}

	public void setIsCreatorName(String isCreatorName) {
		this.isCreatorName = isCreatorName;
	}

	public Integer getClosed() {
		return closed = closed ==null?0:closed ;
	}

	public void setClosed(Integer closed) {
		this.closed = closed ==null?0:closed ;
	}

	public String getClosedName() {
		return closedName;
	}

	public void setClosedName(String closedName) {
		this.closedName = closedName;
	}

	public String getCloseStatu() {
		return closeStatu;
	}

	public void setCloseStatu(String closeStatu) {
		this.closeStatu = closeStatu;
	}

	public Integer getRoleID() {
		return roleID;
	}

	public void setRoleID(Integer roleID) {
		this.roleID = roleID;
	}

	public String getRoleName() {
		return roleName;
	}

	public void setRoleName(String roleName) {
		this.roleName = roleName;
	}

	public String getPositionRoleName() {
		return positionRoleName;
	}

	public void setPositionRoleName(String positionRoleName) {
		this.positionRoleName = positionRoleName;
	}
	

}
