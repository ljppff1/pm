package com.superdata.pm.entity;

import java.util.Date;

/**
 * 企业文库实体类
 * @ClassName IFiles
 * @author lyyong
 * @date 2014年8月6日 上午11:13:01
 *
 */
public class IFiles {
    private Integer id;			//文档ID

    private Integer companyId;	//企业ID

    private String code;		//文档编号

    private String name;		//文档名称

    private Integer typeId;		//文档类型ID
    
    private String typeName;	//文档类型名称

    private Byte attachment;	//附件数

    private Integer createId;	//创建人ID
    
    private String createName;	//创建人名称

    private String createDate;	//创建时间

    private Integer updateId;	//修改人ID
    
    private String updateName;	//修改人名称

    private Date updateDate;	//修改时间

    private String content;		//文档内容
    

    public IFiles() {
		super();
	}
    
	public IFiles(Integer id, String code, String name, String typeName,
			String createName, String createDate, String content) {
		super();
		this.id = id;
		this.code = code;
		this.name = name;
		this.typeName = typeName;
		this.createName = createName;
		this.createDate = createDate;
		this.content = content;
	}

	public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

	public Integer getCompanyId() {
		return companyId;
	}

	public void setCompanyId(Integer companyId) {
		this.companyId = companyId;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getTypeId() {
		return typeId;
	}

	public void setTypeId(Integer typeId) {
		this.typeId = typeId;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}

	public Byte getAttachment() {
		return attachment;
	}

	public void setAttachment(Byte attachment) {
		this.attachment = attachment;
	}

	public Integer getCreateId() {
		return createId;
	}

	public void setCreateId(Integer createId) {
		this.createId = createId;
	}

	public String getCreateName() {
		return createName;
	}

	public void setCreateName(String createName) {
		this.createName = createName;
	}

	public String getCreateDate() {
		return createDate;
	}

	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}

	public Integer getUpdateId() {
		return updateId;
	}

	public void setUpdateId(Integer updateId) {
		this.updateId = updateId;
	}

	public String getUpdateName() {
		return updateName;
	}

	public void setUpdateName(String updateName) {
		this.updateName = updateName;
	}

	public Date getUpdateDate() {
		return updateDate;
	}

	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

}